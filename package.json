{
  "name": "lit-element-starter-ts",
  "version": "0.0.0",
  "description": "A simple web component",
  "main": "my-app.js",
  "module": "my-app.js",
  "type": "module",
  "scripts": {
    "dev": "npm run build:watch & npm run serve & npm run copy:styles",
    "copy:styles": "cp ./src/styles.css ./dist/styles.css",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier src/* --write",
    "serve": "npm run serve:frontend & npm run serve:backend",
    "serve:frontend": "es-dev-server --node-resolve --watch --app-index=index.html",
    "serve:backend": "firebase emulators:start --import=seed",
    "test": "mocha --exit"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript"
  ],
  "author": "The Polymer Authors",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@firebase/testing": "^0.20.11",
    "firebase": "^8.2.2",
    "firebase-admin": "^9.4.2",
    "lit-element": "^2.3.1",
    "path-to-regexp": "^6.2.0",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.3.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.46.1",
    "eslint": "^6.8.0",
    "lit-analyzer": "^1.1.10",
    "mocha": "^7.2.0",
    "prettier": "^2.0.4",
    "rollup": "^1.32.1",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "typescript": "^3.8.3"
  }
}
